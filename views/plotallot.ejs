<!DOCTYPE html>

<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="style2.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>

</head>

<body>
    <!-- multistep form -->
    <form id="msform" action="/plotallot" method="POST">
        <!-- progressbar -->
        <ul id="progressbar">
            <li class="active">Plot Allotment</li>
        </ul>
        <!-- fieldsets -->
        <fieldset>
            <h2 class="fs-title">Description</h2>
            <!-- <input type="text" name="id" placeholder="Area Id" /> -->
            <select name="id" placeholder="Select Associate" style="padding: 11px 108px; margin-bottom: 13px;">
                <% plot.forEach(function(plot){%>
                    <option value="<%=plot.id%>">
                        <%=plot.id%>
                    </option>
                    <% })%>
            </select>
            <!-- <label for="fname" style="text-align: left;">Alloted plotociate</label> -->
            <select name="name" placeholder="Select Associate" style="padding: 11px 61px; margin-bottom: 13px;">
                <% ass.forEach(function(ass){%>
                    <option value="<%=ass.name%>">
                        <%=ass.name%>
                    </option>
                    <% })%>
            </select>
            <input type="text" name="percentage" placeholder="Percentage of associate" />
            <input type="text" name="amount" placeholder="Rate Per square feet" />
            <input type="text" name="plotsize" placeholder="Area of Plot" />
            <button type="submit" class="action-button">Submit</button>
        </fieldset>

    </form>
    <script src="app.js"></script>
</body>

</html>